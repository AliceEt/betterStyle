{
  "env": {
    "vue/setup-compiler-macros": true
  },
  "extends": ["plugin:vue/vue3-essential", "standard"],
  "parserOptions": {
    "ecmaVersion": 13,
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "plugins": ["vue", "@typescript-eslint"],
  "rules": {
    "vue/multi-word-component-names": "off"
  }
}
